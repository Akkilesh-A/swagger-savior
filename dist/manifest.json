{
    "manifest_version": 3,
    "name": "Swagger Auth Help",
    "version": "1.0.0",
    "description": "A basic extension",
    "icons":{
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "64": "images/icon-64.png",
        "128": "images/icon-128.png"
    },
    "action":{
        "default_popup":"index.html"
    },
    "permissions":[
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
}